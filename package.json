{
  "name": "@mootable/hashmap",
  "source": "src/index.js",
  "main": "dist/hashmap.cjs",
  "module": "dist/hashmap.mjs",
  "umd:main": "dist/hashmap.umd.js",
  "unpkg": "dist/hashmap.umd.min.js",
  "browser": "dist/hashmap.min.js",
  "exports": {
    "import": "./dist/hashmap.mjs",
    "require": "./dist/hashmap.cjs"
  },
  "version": "0.12.6",
  "description": "HashMap, LinkedHashMap and Higher Order Functions for all other Iterables, such as Map, Set and Array",
  "keywords": [
    "mootable",
    "hashmap",
    "linkedhashmap",
    "higherorder",
    "map",
    "set",
    "collect",
    "collection",
    "iterable",
    "iterator",
    "reduce",
    "filter",
    "object",
    "array",
    "associative",
    "javascript",
    "nodejs",
    "node",
    "browser"
  ],
  "author": "Jack Moxley (https://github.com/jackmoxley)",
  "license": "MIT",
  "homepage": "https://github.com/mootable/hashmap",
  "bugs": {
    "url": "https://github.com/mootable/hashmap/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mootable/hashmap.git"
  },
  "scripts": {
    "build": "npm run build:transpile && npm run build:jsdocs",
    "build:transpile": "rollup -c",
    "build:jsdocs": "jsdoc -c jsdoc-config.json",
    "dev": "rollup -c -w",
    "test": "nyc --cache false --nycrc-path .nycrc mocha --config .mocharc.cjs",
    "test:dist": "nyc --cache false --nycrc-path ./testconfig/dist.nycrc npm-run-all --parallel test:dist:**",
    "test:all": "nyc --cache false --nycrc-path ./testconfig/all.nycrc npm-run-all --parallel test:src --parallel test:dist:**",
    "test:src": "mocha --config testconfig/src.mocharc.cjs",
    "test:dist:default": "mocha --config testconfig/dist.mocharc.cjs",
    "test:dist:min": "mocha --config testconfig/dist.min.mocharc.cjs",
    "test:dist:umd": "mocha --config testconfig/dist.umd.mocharc.cjs",
    "test:dist:umd:min": "mocha --config testconfig/dist.umd.min.mocharc.cjs",
    "test:dist:mjs": "mocha --require esm  --config testconfig/dist.mjs.mocharc.cjs",
    "test:dist:cjs": "mocha --config testconfig/dist.cjs.mocharc.cjs",
    "benchmark": "node benchmark/benchmark.js",
    "lint": "jshint src/hashmap.js test/testHashMap.cjs",
    "precommit": "npm run lint",
    "prepush": "npm run test -- --reporter dot"
  },
  "engines": {
    "node": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-transform-regenerator": "^7.12.13",
    "@babel/preset-env": "^7.13.10",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "benchmark": "^2.1.4",
    "chai": "4.1.1",
    "chalk": "^2.4.2",
    "core-js": "^3.9.1",
    "docdash": "^1.2.0",
    "esm": "^3.2.25",
    "flesler-hashmap": "npm:hashmap@^2.4.0",
    "jsdoc": "^3.6.6",
    "jsdoc-escape-at": "^1.0.1",
    "jshint": "^2.12.0",
    "lodash": "^4.17.21",
    "mocha": "^8.3.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "random-seed": "^0.3.0",
    "regenerator-runtime": "^0.13.7",
    "rollup": "^2.42.1",
    "rollup-plugin-terser": "^7.0.2"
  },
  "directories": {
    "test": "test",
    "benchmark": "benchmark",
    "docs": "docs",
    "dist": "dist",
    "src": "src"
  }
}
