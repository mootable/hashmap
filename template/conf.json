{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "systemName"            : "Mootable",
    "copyright"             : "(c) 2021 Jack Moxley https://github.com/jackmoxley",
    "includeDate"           : false ,
    "theme"                 : "slate",
    "outputSourceFiles"     : false ,
    "outputSourcePath"      : true ,
    "syntaxTheme"           : "dark",
    "sort"                  : true,
    "search"                : true
  }
}